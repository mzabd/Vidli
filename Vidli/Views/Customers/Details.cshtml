@model Vidli.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.MembershipType.MembershipName</li>
    @if (@Model.BirthdDate.HasValue)
    {
        <li>BirthDate: @Model.BirthdDate</li>
    }
    
</ul>

